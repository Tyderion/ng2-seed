{
  "name": "ng2-ts-boilerplate",
  "version": "0.0.0",
  "description": "A simple starter Angular2 project",
  "scripts": {
    "start": "npm run server:dev",
    "server": "npm run server:dev",
    "server:dev": "./node_modules/.bin/webpack-dev-server --host 0.0.0.0 --progress --profile --colors --display-error-details --inline --content-base src/ --env",
    "server:prod": "./node_modules/.bin/http-server dist --cors",
    "server:rest": "./node_modules/.bin/json-server --watch db.json --port 3004",
    "build": "npm run build:dev",
    "build:dev": "./node_modules/.bin/webpack --config webpack.build.js --progress --profile --colors --display-error-details --display-cached --env",
    "build:prod": "./node_modules/.bin/webpack --config webpack.build.js --progress --profile --colors --display-error-details --display-cached --env prod",
    "test": "./node_modules/.bin/karma start",
    "test:live": "./node_modules/.bin/karma start --auto-watch --no-single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w3tecch/ng2-ts-boilerplate"
  },
  "keywords": [
    "Angular2",
    "ng2-seed"
  ],
  "author": "David Weber <david.weber@w3tec.ch>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/w3tecch/ng2-ts-boilerplate/issues"
  },
  "homepage": "https://github.com/w3tecch/ng2-ts-boilerplate#readme",
  "dependencies": {
    "angular2": "2.0.0-beta.0",
    "bootstrap-sass": "^3.3.6",
    "es6-promise": "3.0.2",
    "es6-shim": "0.33.3",
    "moment": "^2.11.2",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.0",
    "zone.js": "0.5.10",
    "jquery": "^2.2.3"
  },
  "devDependencies": {
    "assert": "^1.3.0",
    "awesome-typescript-loader": "^0.17.0",
    "bootstrap-sass": "^3.3.6",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.1.0",
    "chalk": "^1.1.1",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "fs-extra": "^0.26.5",
    "gulp": "^3.8.11",
    "gulp-connect": "^2.3.1",
    "gulp-karma": "0.0.5",
    "gulp-task-listing": "^1.0.1",
    "gulp-typedoc": "^1.2.1",
    "html-webpack-plugin": "^2.7.2",
    "json-loader": "^0.5.4",
    "karma": "^0.13.21",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.3",
    "karma-coverage": "^0.5.3",
    "karma-growl-reporter": "^0.1.1",
    "karma-jasmine": "^0.3.7",
    "karma-mocha-reporter": "^1.2.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.23",
    "karma-typescript-preprocessor2": "^1.1.1",
    "karma-webpack": "^1.7.0",
    "lodash": "^3.10.1",
    "lodash.merge": "^4.3.1",
    "node-sass": "^3.4.2",
    "phantomjs": "^1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "phantomjs-prebuilt": "^2.1.4",
    "postcss-loader": "^0.8.0",
    "precss": "^1.4.0",
    "protractor": "^3.0.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^3.1.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.6.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.7.2",
    "tslint": "^3.5.0",
    "tslint-loader": "^2.1.0",
    "typedoc": "^0.3.12",
    "typescript": "1.7.5",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "webpack-validator": "^1.0.0-beta.4",
    "yargs": "^4.2.0",
    "expose-loader": "^0.7.1",
    "html-loader": "^0.4.3",
    "http-server": "^0.9.0",
    "json-server": "^0.8.10",
    "url-loader": "^0.5.7"
  }
}
